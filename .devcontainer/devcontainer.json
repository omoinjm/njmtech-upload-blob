{
  "name": "Vercel Upload Blob",
  "build": {
    "dockerfile": "Dockerfile",
    "context": "..",
    "args": {
      "VARIANT": "3"
    }
  },
  "workspaceFolder": "/workspaces//njmtech-upload-blob",
  "postCreateCommand": "bash .devcontainer/scripts/post-create-commands.sh",
  "postAttachCommand": "bash .devcontainer/scripts/post-attach-commands.sh",
  "customizations": {
    "vscode": {
      "settings": {
        "terminal.integrated.shell.linux": "/bin/bash",
        "python.pythonPath": "/usr/local/bin/python",
        "python.linting.enabled": true,
        "python.linting.pylintEnabled": true,
        "python.formatting.autopep8Path": "/usr/local/py-utils/bin/autopep8",
        "python.formatting.blackPath": "/usr/local/py-utils/bin/black",
        "python.formatting.yapfPath": "/usr/local/py-utils/bin/yapf",
        "python.linting.banditPath": "/usr/local/py-utils/bin/bandit",
        "python.linting.flake8Path": "/usr/local/py-utils/bin/flake8",
        "python.linting.mypyPath": "/usr/local/py-utils/bin/mypy",
        "python.linting.pycodestylePath": "/usr/local/py-utils/bin/pycodestyle",
        "python.linting.pydocstylePath": "/usr/local/py-utils/bin/pydocstyle",
        "python.linting.pylintPath": "/usr/local/py-utils/bin/pylint",
        "workbench.colorTheme": "Andromeda",
        "workbench.iconTheme": "material-icon-theme",
        "git.decorations.enabled": true,
        "git.autofetch": true,
        "git.confirmSync": false,
        "git.enableSmartCommit": true
      },
      "extensions": [
        "ms-python.python",
        "dbaeumer.vscode-eslint",
        "esbenp.prettier-vscode",
        "ms-azuretools.vscode-bicep",
        "ms-azuretools.vscode-docker",
        "github.copilot",
        "github.codespaces",
        "hermitter.oh-lucy-vscode",
        "pkief.material-icon-theme"
      ]
    }
  },
  "runArgs": ["--env-file", ".devcontainer/.env", "--privileged"],
  "mounts": [
    "source=${localEnv:HOME}/.ssh,target=/home/vscode/.ssh,type=bind,consistency=cached",
    "source=vercel-blob-bashhistory,target=/commandhistory,type=volume"
  ],
  "remoteUser": "vscode"
}
